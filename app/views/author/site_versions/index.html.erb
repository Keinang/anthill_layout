<p id="notice"><%= notice %></p>

<h1>Listing Author Site Versions</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @author_site_versions.each do |author_site_version| %>
      <tr>
        <td><%= link_to 'Show', author_site_version %></td>
        <td><%= link_to 'Edit', edit_author_site_version_path(author_site_version) %></td>
        <td><%= link_to 'Destroy', author_site_version, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Site version', new_author_site_version_path %>
