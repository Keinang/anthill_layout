<h1>Listing author_histories</h1>

<table>
  <thead>
    <tr>
      <th>Site</th>
      <th>Data</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @author_histories.each do |author_history| %>
      <tr>
        <td><%= author_history.site_id %></td>
        <td><%= author_history.data %></td>
        <td><%= link_to 'Show', author_history %></td>
        <td><%= link_to 'Edit', edit_author_history_path(author_history) %></td>
        <td><%= link_to 'Destroy', author_history, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New History', new_author_history_path %>
