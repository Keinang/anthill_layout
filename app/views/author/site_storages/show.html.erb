<% key = @author_site_storage.try(:key) %>
<% if key.nil? %>
    <p>Undefined site key</p>
<% else %>
    <%= stylesheet_link_tag(
                stylesheet_path("../../assets/public/#{key}/css/general")
        ) %>
    <%= javascript_include_tag(
                'scripts/core/lib/require',
                'data-resource' => key,
                'data-global' => true,
                'data-mode' => @author_site_storage.author_site_type.try(:name),
                'data-main' => javascript_path('public/init')
        ) %>
<% end %>